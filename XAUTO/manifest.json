{
"name": "X-AUTO BOT AUTO GOLIKE",
"description": "Auto Click là công cụ hỗ trợ Tự Động kiếm tiền online",
"version": "1.0.0",
"manifest_version": 2,
"icons": { 
		  "16": "img/icon.png",
          "48": "img/icon_48.png",
          "128": "img/icon_128.png" },
  "browser_action": {
      "default_title": "AUTO GOLIKE",
      "default_icon": "img/icon_128.png",
      "default_popup": "popup.html"
  },
"content_scripts":[{         
"matches": ["*://*.golike.mobi/*","*://*.golike.net/*"], 
"js": ["js/content.js","js/jquery.js"]}]
}